{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Kiwi",
  "version": "1.0.0",
  "identifier": "kiwi.biexi.com",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://127.0.0.1:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Kiwi",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "assets/app/config.toml": "config.toml",
      "assets/zip/": "zip/",
      "assets/python/wheels/*.whl": "python/wheels/",
      "assets/python/project_template/": "python/project_template/"
    }
  }
}
