{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "kiwi-app",
  "version": "1.2.3",
  "identifier": "kiwi.biexi.com",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://127.0.0.1:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "kiwi-app",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "assets/app/config.toml": "config.toml",
      "assets/zip/": "zip/",
      "assets/python/wheels/*.whl": "python/wheels/",
      "assets/python/project_template/": "python/project_template/"
    },
    "createUpdaterArtifacts": true
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEM1NDczRDczQjgyOUM3MkQKUldRdHh5bTRjejFIeFNLeDJ0eTJwT1JqNmVQVUpVclRGSi9zWkZxZ2Y5aXN5SzI2eEhIanpxTTgK",
      "endpoints": [
        "https://kiwi.biexi.com/latest.json"
      ]
    }
  }
}